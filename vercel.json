{
    "version": 2,
    "builds": [
      { "src": "next.config.js", "use": "@vercel/next" },
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/socket.io", "dest": "/server.js" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  